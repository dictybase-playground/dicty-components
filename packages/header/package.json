{
  "name": "@dictyBase/header",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "BSD-2-Clause",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react --inject react-shim.js",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react --inject react-shim.js",
    "lint": "TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "~5.16.1",
    "@testing-library/react": "~12.1.2",
    "@testing-library/user-event": "~13.5.0",
    "@types/jest": "~27.4.0",
    "@types/react": "~17.0.39",
    "@types/react-dom": "~17.0.11",
    "config": "*",
    "eslint": "~7.32.0",
    "jest": "~27.4.7",
    "react": "~17.0.2",
    "scripts": "*",
    "tsconfig": "*",
    "tsup": "~5.10.1",
    "typescript": "~4.5.3",
    "@swc/core": "~1.2.152",
    "@swc/jest": "~0.2.20"
  },
  "dependencies": {
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "@fontsource/roboto": "~4.5.2",
    "@material-ui/core": "~4.12.4",
    "@material-ui/icons": "~4.11.2",
    "fp-ts": "~2.12.1",
    "newtype-ts": "~0.3.5",
    "monocle-ts": "~2.3.13",
    "fp-ts-std": "~0.13.1"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "testPathIgnorePatterns": [
      "src/e2e/"
    ],
    "coveragePathIgnorePatterns": [
      "dist/",
      "src/data/",
      "src/types/",
      "src/e2e/",
      "src/styles/"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "moduleNameMapper": {
      "fp-ts-std/(.*)": "fp-ts-std/dist/cjs/$1"
    }
  }
}
